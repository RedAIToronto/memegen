{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "@/hooks/*": ["hooks/*"],
      "@/utils/*": ["utils/*"],
      "@/ui/*": ["components/ui/*"]
    },
    "jsx": "react-jsx"
  },
  "include": ["**/*.js", "**/*.jsx"]
}
